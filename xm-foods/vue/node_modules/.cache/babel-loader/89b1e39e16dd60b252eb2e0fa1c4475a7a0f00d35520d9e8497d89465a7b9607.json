{"ast":null,"code":"export default {\n  name: \"Foods\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      name: null,\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      ids: [],\n      materialVisible: false,\n      materialList: [],\n      stepVisible: false,\n      stepList: []\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    changeStatus(row, status) {\n      row.status = status;\n      this.$request.put('/foods/update', row).then(res => {\n        if (res.code === '200') {\n          // 表示操作成功\n          this.$message.success('操作成功');\n          this.load(1);\n        } else {\n          this.$message.error(res.msg); // 弹出错误的信息\n        }\n      });\n    },\n    viewMaterial(material) {\n      this.materialList = JSON.parse(material || '[]');\n      this.materialVisible = true;\n    },\n    viewSteps(steps) {\n      this.stepList = JSON.parse(steps || '[]');\n      this.stepVisible = true;\n    },\n    del(id) {\n      // 单个删除\n      this.$confirm('您确定删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/foods/delete/' + id).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    handleSelectionChange(rows) {\n      // 当前选中的所有的行数据\n      this.ids = rows.map(v => v.id); //  [1,2]\n    },\n    delBatch() {\n      // 批量删除\n      if (!this.ids.length) {\n        this.$message.warning('请选择数据');\n        return;\n      }\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/foods/delete/batch', {\n          data: this.ids\n        }).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/foods/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","user","JSON","parse","localStorage","getItem","ids","materialVisible","materialList","stepVisible","stepList","created","load","methods","changeStatus","row","status","$request","put","then","res","code","$message","success","error","msg","viewMaterial","material","viewSteps","steps","del","id","$confirm","type","response","delete","catch","handleSelectionChange","rows","map","v","delBatch","length","warning","get","params","list","reset","handleCurrentChange"],"sources":["src/views/manager/Foods.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入名称查询\" style=\"width: 200px\" v-model=\"name\"></el-input>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"operation\">\r\n      <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" strip @selection-change=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"id\" label=\"序号\" width=\"70\" align=\"center\" sortable></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\r\n        <el-table-column label=\"封面\">\r\n          <template v-slot=\"scope\">\r\n            <el-image :src=\"scope.row.cover\" style=\"width: 50px; height: 30px; border-radius: 5px\"\r\n                      :preview-src-list=\"[scope.row.cover]\"></el-image>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"descr\" label=\"简介\"  show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"skills\" label=\"技巧\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column label=\"查看原料\" width=\"100\">\r\n          <template v-slot=\"scope\">\r\n            <el-button size=\"mini\" @click=\"viewMaterial(scope.row.material)\">查看原料</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"查看步骤\" width=\"100\">\r\n          <template v-slot=\"scope\">\r\n            <el-button size=\"mini\" @click=\"viewSteps(scope.row.steps)\">查看步骤</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"userId\" label=\"作者ID\"></el-table-column>\r\n        <el-table-column prop=\"userName\" label=\"作者\"></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"上传时间\"></el-table-column>\r\n        <el-table-column prop=\"count\" label=\"浏览量\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"审核状态\"></el-table-column>\r\n        <el-table-column label=\"审核\" align=\"center\" width=\"220\" fixed=\"right\">\r\n          <template v-slot=\"scope\">\r\n            <el-button :disabled=\"scope.row.status !== '待审核'\" type=\"primary\" size=\"mini\" @click=\"changeStatus(scope.row, '审核通过')\">通过</el-button>\r\n            <el-button :disabled=\"scope.row.status !== '待审核'\" type=\"danger\" size=\"mini\" @click=\"changeStatus(scope.row, '审核拒绝')\">拒绝</el-button>\r\n            <el-button :disabled=\"scope.row.status === '待审核'\" type=\"info\" size=\"mini\" @click=\"changeStatus(scope.row, '待审核')\">重置</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" align=\"center\" width=\"80\" fixed=\"right\">\r\n          <template v-slot=\"scope\">\r\n            <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n    <el-dialog title=\"原料\" :visible.sync=\"materialVisible\" width=\"30%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <div style=\"padding: 0 40px\">\r\n        <div v-for=\"(item, index) in materialList\" :key=\"index\" style=\"display: flex; padding: 10px 0; margin-bottom: 10px; border-bottom: 1px solid #eee\">\r\n          <div style=\"flex: 1\">{{ item.name }}</div>\r\n          <div style=\"flex: 1\">{{ item.value }}</div>\r\n        </div>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"materialVisible = false\">关 闭</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"步骤\" :visible.sync=\"stepVisible\" width=\"30%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <div style=\"padding: 0 40px\">\r\n        <div v-for=\"(item, index) in stepList\" :key=\"index\" style=\"display: flex; padding: 10px 0; margin-bottom: 10px; border-bottom: 1px solid #eee\">\r\n          <div style=\"flex: 1; text-align: center\"><img :src=\"item.img\" alt=\"\" style=\"width: 100px; height: 100px\"></div>\r\n          <div style=\"flex: 1\">\r\n            <div style=\"margin-bottom: 10px; font-weight: bold\">步骤{{ index + 1 }}</div>\r\n            <div>{{ item.text }}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"stepVisible = false\">关 闭</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Foods\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      name: null,\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      ids: [],\r\n      materialVisible: false,\r\n      materialList: [],\r\n      stepVisible: false,\r\n      stepList: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods: {\r\n    changeStatus(row, status) {\r\n      row.status = status\r\n      this.$request.put('/foods/update', row).then(res => {\r\n        if (res.code === '200') {   // 表示操作成功\r\n          this.$message.success('操作成功')\r\n          this.load(1)\r\n        } else {\r\n          this.$message.error(res.msg)  // 弹出错误的信息\r\n        }\r\n      })\r\n    },\r\n    viewMaterial(material) {\r\n      this.materialList = JSON.parse(material || '[]')\r\n      this.materialVisible = true\r\n    },\r\n    viewSteps(steps) {\r\n      this.stepList = JSON.parse(steps || '[]')\r\n      this.stepVisible = true\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/foods/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    handleSelectionChange(rows) {   // 当前选中的所有的行数据\r\n      this.ids = rows.map(v => v.id)   //  [1,2]\r\n    },\r\n    delBatch() {   // 批量删除\r\n      if (!this.ids.length) {\r\n        this.$message.warning('请选择数据')\r\n        return\r\n      }\r\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/foods/delete/batch', {data: this.ids}).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/foods/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAkGA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAL,IAAA;MACAM,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,GAAA;MACAC,eAAA;MACAC,YAAA;MACAC,WAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,aAAAC,GAAA,EAAAC,MAAA;MACAD,GAAA,CAAAC,MAAA,GAAAA,MAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,kBAAAH,GAAA,EAAAI,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAX,IAAA;QACA;UACA,KAAAU,QAAA,CAA